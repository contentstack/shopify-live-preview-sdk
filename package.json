{
  "name": "@contentstack/shopify-live-preview-sdk",
  "version": "1.1.0",
  "description": "Utility functions for Shopify live preview functionality",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && cpy 'src/setup/*.{js,d.ts}' dist/setup",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepack": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "tsc --watch"
  },
  "keywords": [
    "shopify",
    "live-preview",
    "utilities"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@octokit/rest": "^21.1.1",
    "axios": "^1.12.0",
    "express": "^4.18.3",
    "liquidjs": "^10.10.1",
    "simple-git": "^3.22.0",
    "adm-zip": "^0.5.10"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/adm-zip": "^0.5.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/node": "^20.17.48",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "dotenv": "^16.5.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.3.3",
    "cpy-cli": "^5.0.0"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/shopify-live-preview-sdk.git"
  }
}
